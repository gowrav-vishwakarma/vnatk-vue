{
  "name": "vnatk-vue",
  "version": "0.0.40",
  "description": "VNATK-VUE is Frontend part of VNATK (Vue Node AgileTool Kit). Making app development easy by including some best practices as integral part, yet, giving you total customization options.",
  "main": "dist/vnatkvue.ssr.js",
  "browser": "dist/vnatkvue.esm.js",
  "module": "dist/vnatkvue.esm.js",
  "unpkg": "dist/vnatkvue.min.js",
  "contributors": [
    "Gowrav Vishwakarma <gowravvishwakarma@gmail.com>"
  ],
  "license": "MIT",
  "repository": "gowrav-vishwakarma/vnatk-vue",
  "keywords": [
    "vue",
    "vuetify",
    "crud",
    "RAD"
  ],
  "files": [
    "dist/*",
    "src/**/*.vue",
    "src/**/*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "vue-papa-parse": "^2.0.0",
    "vuetify-form-base": "^0.3.3",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@mdi/font": "^5.9.55",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "install": "^0.13.0",
    "minimist": "^1.2.5",
    "npm": "^6.14.11",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "vue": "^2.6.11",
    "vue-router": "^3.4.9",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.11",
    "webpack": "^4.46.0",
    "lodash": "^4.17.19"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vuetify": "^2.2.11",
    "vue-papa-parse": "^2.0.0",
    "vuetify-form-base": "^0.3.3",
    "lodash": "^4.17.19"
  },
  "engines": {
    "node": ">=10"
  }
}
